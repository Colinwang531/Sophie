    "version": "0.2.0",
    "configurations": [
        {
            "name": "DAS",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/x64/Debug/das",
            "args": ["-l", "61001"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            //"preLaunchTask": "DAS",
            "miDebuggerPath": "/usr/bin/gdb"
        },
        {
            "name": "DHS",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/x64/Debug/dhs",
            "args": ["-x", "127.0.0.1", "-p", "61001", "-m", "60927", "-n", "DHS"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
